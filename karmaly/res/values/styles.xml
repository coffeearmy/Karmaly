<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light">
        <item name="android:textColor">#1C1C1C</item>
    </style>
     <style name="ListTheme">
        <item name="android:textColor">#1C1C1C</item>
    </style>
     <style name="NumberText" parent="android:attr/textAppearanceMedium">
        <item name="android:textSize">20sp</item>        
    </style>
    <style name="TaskText">
        <item name="android:textSize">20sp</item>        
    </style>
    <color name="Green_light">#ff99cc00</color>
    <color name="Red_light">#ffff4444</color>
	<color name="Dark_bg_lv">#EAEAEA</color>
	<color name="Light_bg_lv">#F6F6F6</color>
	<color name="Bg_bg_lv">#E9ECEF</color>
	<color name="Dark_Bg_bg_lv">#c5bec4</color>
	<color name="Tabs_color">#333434</color>
	
</resources>